.header{
    height: 400rpx;
    width:100%;
    position:relative;
}
.header_logo{
    width:100%;
    height:100%;
}
.header_head{
    width:180rpx;
    height:180rpx;
    position:absolute;
    right:50rpx;
    bottom:-50rpx;
}
.search-wrap{
    padding-top:40rpx;
}
.search_button{
    margin-top:40rpx;
    background-color:#ff6d00;
    border: none;
    display:block;
    width:50%;
    border-radius:999px;
}
.search_input-view{
    height:88rpx;
    width:90%;
    background:#fff;
    line-height:88rpx;
    padding-left:32rpx;
    color:#666;
}


